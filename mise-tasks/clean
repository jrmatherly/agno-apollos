#!/usr/bin/env bash
#MISE description="Clean build artifacts and caches"
set -e

echo "Cleaning..."

rm -rf .venv/
rm -rf .mypy_cache/
rm -rf .ruff_cache/
rm -rf .pytest_cache/
rm -rf __pycache__/
find . -type d -name "__pycache__" -exec rm -rf {} + 2>/dev/null || true
rm -rf build/ dist/ *.egg-info/

echo "Done."
